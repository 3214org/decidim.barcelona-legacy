<% provide :title do %><%= t('.title') %><% end %>

<div class="page row-full">
  <div class="row">
    <div class="more-information text small-12 medium-8 medium-centered column">
      <h2><%=t '.title' %></h2>
      <ul>
        <li>
          <span>
          <%= link_to download_for(:pam) do %>
            <%=t '.download-pam'%>
          <% end %>
          </span>
        </li>

        <% District.all.each do |district| %>
          <li>
            <span>
              <%= link_to t('.download-district', district: district.name), download_for(district.id) %>
            </span>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
